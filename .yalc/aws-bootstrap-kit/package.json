{
  "name": "aws-bootstrap-kit",
  "version": "0.4.0",
  "repository": {
    "url": "https://github.com/awslabs/aws-bootstrap-kit",
    "type": "git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "license": "Apache-2.0",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "aws-ssabuilders.aws-bootstrap-kit",
        "module": "aws-ssabuilders.aws-bootstrap-kit"
      },
      "java": {
        "package": "software.amazon.awsssabuilders.services.bootstrapKit",
        "maven": {
          "groupId": "software.amazon.awsssabuilders",
          "artifactId": "bootstrap-kit"
        }
      },
      "dotnet": {
        "namespace": "Amazon.SSABUILDERS.AWS.BootstrapKit",
        "packageId": "Amazon.SSABUILDERS.AWS.BootstrapKit"
      }
    }
  },
  "author": {
    "name": "AWS EMEA SA Specialist Builders"
  },
  "scripts": {
    "deploy": "tsc && cdk deploy",
    "cdk": "cdk",
    "build": "tsc && jsii && jsii-docgen",
    "build:watch": "jsii -w",
    "package": "jsii-pacmak --target js",
    "js-package": "jsii-pacmak --target js",
    "test": "tsc && jest",
    "watch": "tsc -w",
    "release": "standard-version",
    "docgen": "jsii-docgen"
  },
  "dependencies": {
    "aws-cdk-lib": "2.1.0",
    "constructs": "10.0.12"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.1.0",
    "constructs": "^10.0.12"
  },
  "yalcSig": "af2b3a4cbf0fa88de7078ad3f679a333"
}
